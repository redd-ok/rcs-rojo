{
    "name": "rcs-rojo",
    "tree": {
        "$className": "DataModel",

        "Workspace": {
            "$ignoreUnknownInstances": true,
            "RCS_Workspace": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "Client": {
                    "$className": "Folder",
                    "$ignoreUnknownInstances": true
                },
                "Server": {
                    "$className": "Folder",
                    "$ignoreUnknownInstances": true
                }
            },
            "$path": "assets/workspace"
        },

        "StarterGui": {
            "$className": "StarterGui",
            "RCS_Hud": {
                "$path": "assets/hud.rbxm"
            }
        },

        "Lighting": {
            "$ignoreUnknownInstances": true,
            "SuppressionBlur": {
                "$className": "BlurEffect",
                "$properties": {
                    "Size": 0
                }
            },
            "MedicalBlurEffect": {
                "$className": "BlurEffect",
                "$properties": {
                    "Size": 0
                }
            },
            "Flashbang": {
                "$className": "ColorCorrectionEffect",
                "$properties": {}
            },
            "MedicalColorEffect": {
                "$className": "ColorCorrectionEffect",
                "$properties": {}
            },

            "$path": "assets/lighting"
        },

        "MaterialService": {
            "$properties": {
                "Use2022Materials": true
            }
        },

        "StarterPack": {
            "$path": "assets/tools"
        },

        "ReplicatedStorage": {
            "RCS": {
                "$className": "Configuration",
                "$ignoreUnknownInstances": true,
                "Animations": {
                    "$className": "Folder",
                    "$path": "assets/animations"
                },
                "Attachments": {
                    "$className": "Folder",
                    "$ignoreUnknownInstances": true
                },
                "FX": {
                    "$path": "assets/effects.rbxm"
                },
                "Footsteps": {
                    "$path": "assets/footsteps.rbxm"
                },
                "GunModels": {
                    "$path": "assets/models"
                },
                "Modules": {
                    "$path": "src/shared"
                },
                "RCS_Config": {
                    "$path": "src/config.luau"
                },
                "Types": {
                    "$path": "src/types.luau"
                },
                "ClientNetwork": {
                    "$path": "src/network/clientNetwork.luau"
                },

                "Arms": {
                    "$path": "assets/arms.rbxm"
                }
            }
        },

        "ServerScriptService": {
            "RCS_Server": {
                "$path": "src/server",

                "ServerNetwork": {
                    "$path": "src/network/serverNetwork.luau"
                }
            }
        },

        "TextChatService": {
            "$properties": {
                "ChatVersion": "TextChatService"
            }
        },

        "StarterPlayer": {
            "StarterPlayerScripts": {
                "$ignoreUnknownInstances": true,
                "RCS_EventHandler": {
                    "$path": "src/client/eventHandler.client.luau"
                },
                "RCS_Debug": {
                    "$path": "src/client/debug.client.luau"
                }
            },
            "StarterCharacterScripts": {
                "RCS_Framework": {
                    "$path": "src/client/framework"
                },
                "Footsteps": {
                    "$path": "src/client/footsteps.client.luau"
                }
            },

            "$properties": {
                "GameSettingsAvatar": "R6"
            }
        }
    }
}
